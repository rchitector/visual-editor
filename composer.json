{
    "name": "rchitector/visual-editor",
    "description": "Visual editor for laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rchitector\\VisualEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rchitector\\VisualEditor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rchitector\\VisualEditor\\App\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
